{
  "kind": "collectionType",
  "collectionName": "product_legals",
  "info": {
    "singularName": "product-legal",
    "pluralName": "product-legals",
    "displayName": "Product Legal"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "medusa_id": {
      "type": "uid"
    },
    "terms-and-conditions": {
      "type": "richtext"
    },
    "privacy-policy": {
      "type": "richtext"
    },
    "return-policy": {
      "type": "richtext"
    },
    "fssai-license": {
      "type": "string",
      "maxLength": 14,
      "minLength": 0
    },
    "last-updated": {
      "type": "date",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "product-legal"
    }
  }
}
